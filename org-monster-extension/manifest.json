{
  "manifest_version": 2,
  "name": "Org-Monster",
  "version": "1.0",
  "description": "A Chrome Extension that will help you follow GitHub Organisations",

  "browser_action": {
    "default_popup": "popup.html"
  },

  "icons":{
    "16": "asset/icon16.png",
    "48": "asset/icon48.png",
    "128":"asset/icon128.png"
  },

  "permissions": [
    "storage"
  ]
}
